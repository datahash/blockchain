{
  "name": "blockchain",
  "description": "Blockchain",
  "author": "Nick Zbiegien",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "server/server.app.js",
  "engines": {
    "node": "12.16.3"
  },
  "scripts": {
    "postinstall": "yarn install:client",
    "install:client": "yarn --cwd client install",
    "start": "yarn build:client && node server/server.app.js",
    "build:client": "yarn --cwd client build",
    "dev": "ENV='development' nodemon server/server.app.js",
    "dev-peer": "GENERATE_PEER_PORT='true' nodemon server/server.app.js",
    "test": "jest --watchAll",
    "ts": "tsc -w"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "bs58": "^4.0.1",
    "crypto-js": "^4.0.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "elliptic": "^6.5.2",
    "express": "^4.17.1",
    "hex-to-binary": "^1.0.1",
    "pubnub": "^4.27.6",
    "ramda": "^0.27.0",
    "request": "^2.88.2",
    "uuid": "^8.1.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
