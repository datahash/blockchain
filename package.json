{
  "name": "blockchain",
  "description": "Private Blockchain",
  "author": "Nick Zbiegien",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "server/server.app.ts",
  "scripts": {
    "start": "node ./build/server/server.app.js",
    "build": "yarn tsc",
    "heroku-postbuild": "yarn tsc && yarn --cwd client install && yarn --cwd client build",
    "dev-client": "yarn --cwd client start",
    "dev-ts": "ENV='development' nodemon",
    "dev": "ENV='development' node build/server/server.app.js",
    "dev-peer": "GENERATE_PEER_PORT='true' nodemon build/server/server.app.js",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "elliptic": "^6.5.2",
    "express": "^4.17.1",
    "hex-to-binary": "^1.0.1",
    "pubnub": "^4.27.6",
    "ramda": "^0.27.0",
    "request": "^2.88.2",
    "socket.io": "^2.4.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/ramda": "^0.27.6",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "bs58": "^4.0.1",
    "crypto-js": "^4.0.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "jest": {
    "testEnvironment": "node"
